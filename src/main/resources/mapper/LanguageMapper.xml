<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuyuan.oracle.mapper.LanguageMapper">

    <resultMap id="BaseResultMap" type="com.liuyuan.oracle.model.entity.Language">
